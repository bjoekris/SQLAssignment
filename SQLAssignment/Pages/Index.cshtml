@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<body class="bg-light">
    <h2>Titles</h2>

    <table class="table table-striped table-hover bg-yellow rounded-2">
        <thead>
            <tr>
                <th>Titles ID</th>
                <th>Title</th>
                <th>StartYear</th
            </tr>
        </thead>
        <tbody>

            @foreach (var vare in Model.Titles)
            {
                <tr>


                    <td>@vare.TitlesID</td>
                    <td>@vare.Title</td>
                    <td>@vare.StartYear</
                </tr>


            }
        </tbody>
    </table>
</body>
